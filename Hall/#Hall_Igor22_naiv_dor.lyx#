\begin_layout Standard
הטמפ' גבוהות ולכן המל
\begin_inset Quotes erd
\end_inset

מ נמצא במשטר האינטרינזי, במשטר זה מתקיים 
\begin_inset Formula 
\[
n=p=n_{i}\sim e^{-\frac{E_{g}}{2k_{B}T}}
\]

\end_inset


\end_layout

\begin_layout Standard
כמו כן 
\begin_inset Formula 
\[
U_{p}=\frac{\rho_{xx}L}{Wd}I_{x},\rho_{xx}=\frac{1}{e\left(n\mu_{e}+\mu_{h}p\right)}=\frac{1}{n_{i}e\left(\mu_{e}+\mu_{h}\right)}
\]

\end_inset


\begin_inset Formula 
\[
\Rightarrow U_{p}^{-1}\sim n_{i}\sim e^{-\frac{E_{g}}{2k_{B}T}}
\]

\end_inset


\end_layout

\begin_layout Standard
בנוסף, בטמפ' אלו מתקיים 
\begin_inset Formula $T^{-1}<<1$
\end_inset

 ולכן ניתן לקרב את האקספוננט לפונקציה לינארית ולקבל 
\begin_inset Formula 
\[
U_{p}^{-1}\sim1-\frac{E_{g}}{2k_{B}}\cdot T^{-1}
\]

\end_inset

 והרי שקיבלנו שבטמפ' גבוהות אכן אמור להתקיים יחס לינארי מקורב בין 
\begin_inset Formula $U_{p}^{-1}$
\end_inset

